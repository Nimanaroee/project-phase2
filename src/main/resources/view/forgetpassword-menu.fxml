<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/forgetpasswordMenu.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ForgetPasswordController">
    <VBox alignment="CENTER" layoutX="204.0" layoutY="8.0" prefHeight="384.0" prefWidth="192.0" spacing="5.0">
        <TextField fx:id="usernameField" alignment="CENTER" promptText="username" />
        <Button fx:id="questionButton" mnemonicParsing="false" onAction="#onClickQuestionButton" prefHeight="83.0" prefWidth="159.0" text="question" />
        <TextField fx:id="answerField" alignment="CENTER" promptText="answer" />
        <Button fx:id="submitAnswerButton" mnemonicParsing="false" onAction="#onClickSubmitAnswer" prefHeight="84.0" prefWidth="152.0" text="submit answer" />
        <Button fx:id="backButton" mnemonicParsing="false" onAction="#onClickBackButton" prefHeight="84.0" prefWidth="148.0" text="back" />
    </VBox>
</AnchorPane>
